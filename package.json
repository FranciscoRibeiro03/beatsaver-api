{
  "name": "beatsaver-api",
  "version": "2.0.2",
  "description": "A BeatSaver API Wrapper",
  "main": "./lib/BeatSaverAPI.js",
  "types": "./lib/BeatSaverAPI.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rui2015/beatsaver-api.git"
  },
  "author": "rui2015",
  "keywords": [
    "beat",
    "saver",
    "saber",
    "beatsaver",
    "beatsaber"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rui2015/beatsaver-api/issues"
  },
  "homepage": "https://github.com/rui2015/beatsaver-api#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "detect-node": "^2.1.0",
    "semver": "^7.3.5",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/detect-node": "^2.0.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.6",
    "@types/semver": "^7.3.7",
    "jest": "^27.2.4",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.3"
  },
  "files": [
    "lib/**/*"
  ]
}
